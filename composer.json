{
    "name": "da/api-client-bundle",
    "type": "symfony-bundle",
    "description": "DaApiClientBundle is a Symfony2's bundle allowing to discuss in a simple and secure way with an API.",
    "keywords": ["api", "client", "bundle", "da", "symfony2", "symfony", "simple"],
    "homepage": "https://github.com/Gnuckorg/DaApiClientBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Prelot",
            "email": "tprelot@gmail.com"
        },
        {
            "name": "Gabriel Bondaz",
            "email": "gabriel.bondaz@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ext-curl": "*",
        "symfony/dependency-injection" : ">=2.1",
        "symfony/config" : ">=2.1",
        "doctrine/doctrine-cache-bundle": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Da\\ApiClientBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Da/ApiClientBundle"
}
